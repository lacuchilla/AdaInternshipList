<h1>Add a Company</h1>
<%= form_for @company, url: {action: "#{@url}"} do |f| %>
  <fieldset>
    <legend> <%= @legend %></legend>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :website %>
    <%= f.text_field :website %>
  </fieldset>
  <%= f.submit %>
<% end %>
