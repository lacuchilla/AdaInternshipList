<h1>Contacts for <%= @specific_company.name %></h1>
  <ul>
  <% @specific_company.people.each do |person| %>
    <li><%= link_to "#{person.name}", "/companies/#{person.company.id}/people/#{person.id}/" %></li>
  <% end %>
  </ul>
<%= link_to "Add a person", "/companies/#{params[:id]}/people/new" %>
